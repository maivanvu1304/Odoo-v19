<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Tender No -->
        <record id="seq_geega_tender" model="ir.sequence">
            <field name="name">Geega Tender</field>
            <field name="code">geega.tender</field>
            <field name="prefix">TND-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
